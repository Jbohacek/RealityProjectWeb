<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewData["Title"] - RealityProjectWeb</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/style.css"/>    
    <link rel="stylesheet" href="~/css/headerStyle.css" />
    <link rel="stylesheet" href="~/css/footerStyle.css" />
    @await RenderSectionAsync("Styles",false)
</head>
<body>
    <header>
        <div class="infoBox">
            <div class="info">
                <img src="~/References/podklady/emailIcon.png" alt="">
                <a href="mailto:info@realintense.org">info@realintense.org</a>
            </div>
            <div class="info">
                <img src="~/References/podklady/phoneIcon.png" alt="">
